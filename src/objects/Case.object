        <formula>IF(OR(VALUE($Label.HAS_Minimum_Required_Letters_Value) &lt;= Letters_of_Support_Uploaded__c, Letters_Of_Support_Received_Offline__c = true), 0, VALUE($Label.HAS_Minimum_Required_Letters_Value) - Letters_of_Support_Uploaded__c)</formula>
